<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="description" content="Foodeiblog Template">
  <meta name="keywords" content="Foodeiblog, unica, creative, html">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="icon" href="/img/icon.png" type="image/png">
  <title>Admin Contact</title>

  <!-- Google Font -->
  <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:300,400,600,700,800,900&display=swap"
    rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Unna:400,700&display=swap" rel="stylesheet">

  <!-- Css Styles -->
  <link rel="stylesheet" href="/css/bootstrap.min.css" type="text/css">
  <link rel="stylesheet" href="/css/font-awesome.min.css" type="text/css">
  <link rel="stylesheet" href="/css/elegant-icons.css" type="text/css">
  <link rel="stylesheet" href="/css/owl.carousel.min.css" type="text/css">
  <link rel="stylesheet" href="/css/slicknav.min.css" type="text/css">
  <link rel="stylesheet" href="/css/style.css" type="text/css">

</head>

<body>
  <header class="header">
  <div class="container">
    <div class="row">
        <div class="col-lg-3 col-md-3">
            <div class="header__btn">
              <form action="/adminlogout" method="POST">
                <button type="submit">Logout</button>
              </form>
            </div>
        </div>
        <div class="col-lg-6 col-md-6">
            <div class="header__logo">
                <h2>Admin Contact Message List</h2>
            </div>
        </div>
        <div class="col-lg-3 col-md-3">
            <div class="header__social">
                <a href="#"><i class="fa fa-facebook"></i></a>
                <a href="#"><i class="fa fa-twitter"></i></a>
                <a href="#"><i class="fa fa-youtube-play"></i></a>
                <a href="#"><i class="fa fa-instagram"></i></a>
                <a href="#"><i class="fa fa-envelope-o"></i></a>
            </div>
        </div>
    </div>
</div>
</header>
<!-- Header Section End -->
<br/>
  <% contacts.forEach((contact)=> { %>
    <section class="single-post spad">
      <div class="container">
        <div class="row d-flex justify-content-center">
          <div class="col-lg-8">
            <div class="single-post__recipe__details">
              <div class="single-post__recipe__details__option">
                <ul>
                  <li>
                    <h5><i class="fa fa-user-o"></i> NAME</h5>
                    <span>
                      <%= contact.name %>
                    </span>
                  </li>
                  <li>
                    <h5><i class="fa fa-clock-o"></i> EMAILS</h5>
                    <span>
                      <%= contact.email %>
                    </span>
                  </li>
                  <li>
                    <h5><i class="fa fa-clock-o"></i> SUBJECT</h5>
                    <span>
                      <%= contact.subject %>
                    </span>
                  </li>

                </ul>
              </div>
              <div class="single-post__recipe__details__indegradients">
                <h5>Message</h5>
                <ul>
                  <li>
                    <%= contact.message %>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <% }); %>

      <!-- Js Plugins -->
      <script src="/js/jquery-3.3.1.min.js"></script>
      <script src="/js/bootstrap.min.js"></script>
      <script src="/js/jquery.slicknav.js"></script>
      <script src="/js/owl.carousel.min.js"></script>
      <script src="/js/main.js"></script>
</body>

</html>